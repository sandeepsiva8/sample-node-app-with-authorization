<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" type="text/css" href="stylesheets/style.css">
  <title>Admin Login</title>
</head>
<body ng-app="LoginApp">
  <div class="login-box">
    <h2>Login</h2>
    <form action="" class="form" ng-controller="LoginController">
      <div class="user-box">
        <input class="input" type="text" ng-model="log.username" required="">
        <label class="label" for="">Username</label>
      </div>
      <div class="user-box">
        <input class="input" type="password" ng-model="log.password" required="">
        <label class="label" for="">Password</label>
      </div>
      <a href="#" ng-click="Login(log)">
        <span></span>
        <span></span>
        <span></span>
        <span></span>
        Submit
      </a>
      <br>
      <a href="/forgot">Forgot Password ?</a>
    </form>
  </div>
</body>
<script type="text/javascript" src="javascripts/angular/angular.min.js"></script>
<script type="text/javascript" src="javascripts/login/login.js"></script>
</html>